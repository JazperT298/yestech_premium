    <fragment
        android:id="@+id/navigation_educator_list"
        android:name="com.theyestech.yestechpremium.fragments.EducatorListFragment"
        android:label="@string/title_educator_list"
        tools:layout="@layout/fragment_educator_list" />

    <fragment
        android:id="@+id/navigation_student_list"
